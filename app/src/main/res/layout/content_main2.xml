<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="kdas.i_nterface.uitest_2.Main2Activity"
    tools:showIn="@layout/activity_main2">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/main2toolbar"
        android:background="@color/colorAccent"/>

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_1"
        app:backgroundTint="@color/some_teal"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:visibility="invisible" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_2"
        app:backgroundTint="@color/some_teal"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/fab_main_1"
        android:layout_alignStart="@+id/fab_main_6" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_3"
        app:backgroundTint="@color/some_teal"
        android:layout_below="@+id/fab_main_2"
        android:layout_centerHorizontal="true"
        android:visibility="invisible" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_4"
        app:backgroundTint="@color/some_teal"
        android:layout_below="@+id/fab_main_3"
        android:layout_alignStart="@+id/fab_main_2" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_5"
        app:backgroundTint="@color/some_teal"
        android:layout_below="@+id/main2toolbar"
        android:layout_alignParentStart="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="46dp" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_6"
        app:backgroundTint="@color/some_teal"
        android:layout_above="@+id/fab_main_1"
        android:layout_alignStart="@+id/fab_main_5" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fab_main_7"
        app:backgroundTint="@color/some_red"
        android:layout_above="@+id/fab_main_3"
        android:layout_alignParentEnd="true"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Initialize Yourself"
        android:fontFamily="sans-serif-thin"
        android:textSize="18sp"
        android:id="@+id/textView9"
        android:layout_alignTop="@+id/fab_main_5"
        android:layout_alignStart="@+id/textView10"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Share Location"
        android:fontFamily="sans-serif-thin"
        android:textSize="18sp"
        android:id="@+id/textView10"
        android:layout_above="@+id/fab_main_1"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Events/Day "
        android:fontFamily="sans-serif-thin"
        android:textSize="18sp"
        android:id="@+id/textView11"
        android:layout_alignBottom="@+id/fab_main_2"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Peers"
        android:fontFamily="sans-serif-thin"
        android:textSize="18sp"
        android:id="@+id/textView12"
        android:layout_alignBottom="@+id/fab_main_4"
        android:layout_alignStart="@+id/textView11"
        android:textStyle="bold" />

